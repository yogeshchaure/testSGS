CREATE TABLE SGS_BUSINESS_UNITS(
	BU_ID int NOT NULL,
	BU_NAME varchar(80) NOT NULL,
	BU_DESC varchar(80) NULL,
	CREATED_DATE date NULL,
	CREATED_BY varchar(80) NULL,
	UPDATED_DATE date NULL,
	UPDATED_BY varchar(80) NULL,
 CONSTRAINT PK_SGS_BUSINESS_UNITS PRIMARY KEY(
	BU_ID ASC
));

CREATE TABLE SGS_SERVICES(
	SERVICES_ID int NOT NULL,
