CREATE TABLE SGS_RESP_FUNCTIONS(
	RESP_FUNCTION_ID int NOT NULL,
	RESPONSIBILITY_ID int NOT NULL,
	FUNCTION_ID int not null,
	STATUS varchar(80) ,
	CREATED_DATE date ,
	CREATED_BY varchar(80) ,
	UPDATED_DATE date,
	UPDATED_BY varchar(80) 
	);

CREATE SEQUENCE SGS_RESP_FUNCTIONS_SEQ
 START WITH 1
 INCREMENT BY 1;

create unique index SGS_RESP_FUNCTIONS_U1 on SGS_RESP_FUNCTIONS(RESP_FUNCTION_ID );
create index SGS_RESP_FUNCTIONS_N1 on SGS_RESP_FUNCTIONS(RESPONSIBILITY_ID);

--compilation doc from K J(Comparison)

create view SGS_GROUP_MEMBER_LIST AS (
    SELECT USER_NAME, FUNCTION_NAME
FROM   (SELECT SRF.RESPONSIBILITY_ID,
                               (SELECT RESPONSIBILITY_NAME
                               FROM    dbo.SGS_RESPONSIBILITIES AS SR
                               WHERE (RESPONSIBILITY_ID = SRF.RESPONSIBILITY_ID)) AS RESP_NAME, SRF.FUNCTION_ID,
                               (SELECT FUNCTION_NAME
                               FROM    dbo.SGS_FUNCTIONS AS SF
                               WHERE (FUNCTION_ID = SRF.FUNCTION_ID)) AS FUNCTION_NAME, UAC.USER_NAME
             FROM    dbo.SGS_RESP_FUNCTIONS AS SRF INNER JOIN
                               (SELECT B.USER_ID, B.RESPONSIBILITY_ID, A.USER_NAME
                               FROM    dbo.SGS_USERS AS A INNER JOIN
                                            dbo.SGS_USER_ACCESS AS B ON A.USER_ID = B.USER_ID AND A.STATUS = 'A') AS UAC ON SRF.RESPONSIBILITY_ID = UAC.RESPONSIBILITY_ID) AS SS
)

