);


CREATE TABLE SGS_ATTACHMENTS(
	ATTACHMENT_ID int NOT NULL,
	DOCUMENT_TYPE varchar(80) NOT NULL, -- DOCUMENT TYPE IS LIKE TABLE NAME e.g. USERS
	DOCUMENT_ID int NOT NULL,-- like USER_ID
	ATTACHMENT_TYPE varchar(80) NOT NULL, -- FILE ATTACHMENT,URL --dropdown
	URL varchar(240) , -- on the basis of ATTACHMENT_TYPE
	BFILE VARBINARY(MAX),
	CREATED_DATE date ,
	CREATED_BY varchar(80) ,
	UPDATED_DATE date,
	UPDATED_BY varchar(80)
);

CREATE SEQUENCE SGS_RESPONSIBILITIES_SEQ
 START WITH 1
 INCREMENT BY 1;

create unique index SGS_RESPONSIBILITIES_U1 on SGS_RESPONSIBILITIES(RESPONSIBILITY_ID);
create unique index SGS_RESPONSIBILITIES_U2 on SGS_RESPONSIBILITIES(RESPONSIBILITY_NAME);
create index SGS_RESPONSIBILITIES_N1 on SGS_RESPONSIBILITIES(DESCRIPTION);

CREATE SEQUENCE SGS_FUNCTIONS_SEQ
 START WITH 1
 INCREMENT BY 1;
create unique index SGS_FUNCTIONS_U1 on SGS_FUNCTIONS(FUNCTION_ID);


